{
  "type": "object",
  "patternProperties": {
    "^[^\\n]+$": {
      "type": "object",
      "properties": {
        "function_name": {
          "type": "string",
          "pattern": "\\S+"
        },
        "api_group": {
          "type": "array",
          "minItems": 1,
          "items": {
            "type": "object",
            "properties": {
              "api": {
                "type": "string",
                "pattern": "\\S+"
              },
              "method": {
                "type": "string",
                "pattern": "\\S+"
              }
            },
            "required": ["api", "method"]
          }
        }
      },
      "required": ["function_name", "api_group"]
    }
  },
  "additionalProperties": false
}
